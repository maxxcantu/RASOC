      <!-- Lidar Sensor -->
      <sensor name="lidar" type="ray">
        <always_on>true</always_on>
        <visualize>true</visualize>
        <pose>0.315 0 0.415 0 0 0</pose>
        <update_rate>1000</update_rate>
        <ray>
          <scan>
            <horizontal>
               <samples>128</samples> 
              <!-- <resolution>0.017453</resolution> -->
              <!-- -60 grados a 60 grados con 1 grado de paso
              <min_angle>-1.047198</min_angle>
              <max_angle>1.047198</max_angle> -->
              <!-- -127grados a 127 grados con 2 grados de paso -->
              <min_angle>-2.21658</min_angle>
              <max_angle>2.21658</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.06</min>
            <max>3</max>
            <resolution>0.15000</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="lidar" filename="libgazebo_ros_ray_sensor.so">
          <ros>
            <namespace>/demo</namespace>
            <argument>--ros-args --remap ~/out:=scan</argument>
          </ros>
          <output_type>sensor_msgs/LaserScan</output_type>
        </plugin>
      </sensor>

      <!-- GPS Sensor -->
      <sensor name="my_gps" type="gps">
