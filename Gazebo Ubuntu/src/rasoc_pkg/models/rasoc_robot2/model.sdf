          <plugin name="rasoc_gps" filename="libgazebo_ros_gps_sensor.so">
            <ros>
              <namespace>/gps_rasoc</namespace>
              <remapping>~/out:=pos</remapping>
            </ros>
            <frame_name>body_robot</frame_name>
          </plugin>
        </sensor>

